{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module":"CommonJS",
    "target": "ES6",
    "lib":["es6"],
    "jsx": "react-native",
    // "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "noImplicitAny": false
  }
}
